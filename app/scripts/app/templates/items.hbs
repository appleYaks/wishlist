<ul class="items">
  {{#each}}
  <li>
    {{input type="checkbox" checked=complete}}
    {{#link-to 'item' this}}
      {{title}}
    {{/link-to}}
    <div class="controls">
      <button {{action 'edit' this}}><i class="fa fa-pencil"></i></button>
      <button {{action 'delete' this}}><i class="fa fa-close"></i></button>
    </div>
  </li>
  {{/each}}
</ul>
